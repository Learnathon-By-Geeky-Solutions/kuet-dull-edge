# .env.example for MinIO configuration

# Root credentials (change these before deploying)
MINIO_ROOT_USER=root
MINIO_ROOT_PASSWORD=changeme

# Optional settings
# Toggle the MinIO browser (set to "on" or "off")
MINIO_BROWSER=on

# Specify the domain (if using custom domain)
MINIO_DOMAIN=example.com

# (Optional) Define additional configuration options as needed below